{
  "name": "kretes",
  "version": "1.0.0-alpha.59",
  "description": "A Programming Environment for TypeScript apps built on top of VS Code",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "repository": "https://github.com/kreteshq/kretes",
  "author": "Zaiste",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 12"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@snowpack/plugin-postcss": "^1.4.3",
    "@types/config": "^0.0.39",
    "@types/fs-extra": "^9.0.12",
    "@types/lru-cache": "^5.1.1",
    "@types/semver": "^7.3.7",
    "@types/yargs": "^17.0.2",
    "ava": "^3.15.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "form-data": "^4.0.0",
    "kretes-snowpack-refresh": "^0.0.4",
    "prettier": "2.3.2"
  },
  "scripts": {
    "prepublishOnly": "tsc && tsc -p http",
    "pretest": "tsc",
    "test": "ava",
    "dev:main": "rm -rf dist && tsc -w",
    "dev:client": "tsc -w -p http",
    "build": "rm -rf dist && tsc -p . && tsc -p http",
    "debug": "node --inspect cli.js",
    "prepack": "node scripts/prepack",
    "clean": "rm -rf dist",
    "lint": "eslint ."
  },
  "dependencies": {
    "@casl/ability": "^5.3.1",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@poppinss/chokidar-ts": "^3.3.1",
    "@sqorn/pg": "0.0.45",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "argon2": "^0.28.2",
    "basic-auth": "^2.0.1",
    "boxwood": "^0.56.1",
    "busboy": "^0.3.1",
    "chalk": "^4.1.1",
    "colorette": "^1.2.2",
    "config": "^3.3.6",
    "cookie": "^0.4.1",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "dumper.js": "^1.3.1",
    "error-stack-parser": "^2.0.6",
    "esbuild": "^0.12.15",
    "fast-glob": "3.2.7",
    "fs-extra": "^10.0.0",
    "graphile-utils": "^4.12.1",
    "graphile-worker": "^0.11.3",
    "graphql": "15.5.1",
    "graphql-tools": "^7.0.5",
    "http-status": "^1.5.0",
    "http-terminator": "^3.0.0",
    "lookpath": "^1.2.1",
    "mime-types": "^2.1.31",
    "pg": "^8.6.0",
    "postcss": "^8.3.6",
    "postgraphile": "^4.12.3",
    "retes": "^0.20.0",
    "rsyncwrapper": "^3.0.1",
    "semver": "^7.3.5",
    "snowpack": "3.6.2",
    "stack-trace": "0.0.10",
    "typescript": "4.3.5",
    "validate": "^5.1.0",
    "ws": "^7.5.3",
    "yargs": "17.0.1"
  },
  "bin": {
    "kretes": "dist/cli.js",
    "ks": "dist/cli.js"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 111,
    "parser": "typescript"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "source/": "dist/"
      }
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./http": "./dist/http/index.js",
    "./request": "./dist/request.js",
    "./route": "./dist/route.js",
    "./response": "./dist/response.js",
    "./auth": "./dist/auth.js"
  },
  "typesVersions": {
    "*": {
      "request": [
        "dist/request"
      ],
      "response": [
        "dist/response"
      ],
      "route": [
        "dist/route"
      ],
      "auth": [
        "dist/auth"
      ]
    }
  },
  "keywords": [
    "kretes",
    "typescript",
    "framework",
    "node.js",
    "web",
    "snowpack",
    "esbuild",
    "api",
    "react",
    "vue",
    "solidjs",
    "front-end"
  ]
}
